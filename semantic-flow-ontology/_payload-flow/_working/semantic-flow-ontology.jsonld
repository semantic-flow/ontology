{
  "@context": {
    "@base": "https://semantic-flow.github.io/ontology/semantic-flow/",
    "owl": "http://www.w3.org/2002/07/owl#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "dcterms": "http://purl.org/dc/terms/",
    "dcat": "http://www.w3.org/ns/dcat#",
    "sh": "http://www.w3.org/ns/shacl#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "prov": "http://www.w3.org/ns/prov#",
    "schema": "https://schema.org/",
    "vann": "http://purl.org/vocab/vann/"
  },
  "@graph": [
    {
      "@id": "/",
      "@type": "owl:Ontology",
      "dcterms:title": "Semantic Flow Ontology",
      "dcterms:description": "The Semantic Flow ontology defines the classes and properties used for defining and describing semantic meshes.",
      "dcterms:creator": {
        "@id": "https://djradon.github.io/ns/djradon/"
      },
      "owl:versionInfo": "0.1.0",
      "owl:versionIRI": {
        "@id": "https://semantic-flow.github.io/ontology/semantic-flow/_payload/_default/sflo.ttl"
      },
      "vann:preferredNamespacePrefix": "sflo",
      "vann:preferredNamespaceUri": "https://semantic-flow.github.io/ontology/semantic-flow/"
    },
    {
      "@id": "Resource/",
      "@type": "rdfs:Class",
      "rdfs:label": "Resource",
      "rdfs:comment": "Abstract base class for all Semantic Flow resources. Provides fundamental identification and documentation affordances."
    },
    {
      "@id": "FileResource/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": {
        "@id": "Component/"
      },
      "rdfs:label": "File Resource",
      "rdfs:comment": "A resource that locates and corresponds directly to a file."
    },
    {
      "@id": "NamingResource/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": {
        "@id": "Resource/"
      },
      "rdfs:label": "Naming Resource",
      "rdfs:comment": "A resource that establishes a name, optionally denoting a non-file thing. Naming resources' IRIs end with a slash and may or may not have a referent."
    },
    {
      "@id": "hasResource/",
      "@type": "owl:ObjectProperty",
      "schema:domainIncludes": {
        "@id": "Resource/"
      },
      "schema:rangeIncludes": {
        "@id": "Resource/"
      },
      "rdfs:label": "has resource",
      "rdfs:comment": "Abstract base property for associating resources."
    },
    {
      "@id": "AssetTree/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": [
        {
          "@id": "Component/"
        },
        {
          "@id": "NamingResource/"
        }
      ],
      "rdfs:label": "AssetTree",
      "rdfs:comment": "Asset trees contain collections of files and directories that are not MeshResources. Can be contained in any NamingResource."
    },
    {
      "@id": "Knop/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": {
        "@id": "NamingResource/"
      },
      "rdfs:label": "Mesh Knop",
      "rdfs:comment": "Structural units that provide identifiers for things, may act as namespace segments to organize/contain other resources, may contain reference data about their referents, and may have a payload dataset. Knops form the structure of the mesh."
    },
    {
      "@id": "BareKnop/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": {
        "@id": "Knop/"
      },
      "rdfs:label": "Namespace",
      "rdfs:comment": "Bare knops - containers for organizing other mesh knops. Knop identifiers refer to the knops themselves as organizational units."
    },
    {
      "@id": "ReferenceKnop/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": {
        "@id": "Knop/"
      },
      "rdfs:label": "Reference",
      "rdfs:comment": "Knops that refer to some external 'thing' or concept other than themselves."
    },
    {
      "@id": "PayloadKnop/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": {
        "@id": "Knop/"
      },
      "rdfs:label": "Data",
      "rdfs:comment": "Knops that contain and refer to 'payload data' via a payload flows."
    },
    {
      "@id": "hasComponent/",
      "@type": "owl:ObjectProperty",
      "rdfs:subPropertyOf": {
        "@id": "hasResource/"
      },
      "rdfs:domain": {
        "@id": "Knop/"
      },
      "rdfs:range": {
        "@id": "Component/"
      },
      "rdfs:label": "has component",
      "rdfs:comment": "Associates knops with their supporting components."
    },
    {
      "@id": "isHandleFor/",
      "@type": "owl:ObjectProperty",
      "owl:inverseOf": {
        "@id": "hasHandle/"
      },
      "rdfs:domain": {
        "@id": "HandleAsComponent/"
      },
      "rdfs:range": {
        "@id": "KnopHandle/"
      },
      "rdfs:label": "is handle for",
      "rdfs:comment": "Indicates that this handle component provides identification and routing for a specific knop."
    },
    {
      "@id": "hasHandle/",
      "@type": "owl:ObjectProperty",
      "rdfs:subPropertyOf": {
        "@id": "hasComponent/"
      },
      "owl:inverseOf": {
        "@id": "isHandleFor/"
      },
      "rdfs:domain": {
        "@id": "KnopHandle/"
      },
      "rdfs:range": {
        "@id": "HandleAsComponent/"
      },
      "rdfs:label": "has handle",
      "rdfs:comment": "Associates a mesh knop (via its handle) with its persistent handle component."
    },
    {
      "@id": "slug/",
      "@type": "owl:DatatypeProperty",
      "schema:domainIncludes": [
        {
          "@id": "KnopHandle/"
        }
      ],
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "rdfs:label": "has slug",
      "rdfs:comment": "Required kebab-case identifier for a Knop, used as the directory segment and URL component. Used in meta flow."
    },
    {
      "@id": "Component/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": {
        "@id": "Resource/"
      },
      "rdfs:label": "Component",
      "rdfs:comment": "Terminal resources that provide specific mesh functionality and support knops."
    },
    {
      "@id": "KnopHandle/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": {
        "@id": "NamingResource/"
      },
      "rdfs:label": "Handle",
      "rdfs:comment": "Conceptual handle that refers to a knop 'as a knop' (as opposed to the thing/concept referred to by the parent knop). This is the abstract handle concept."
    },
    {
      "@id": "HandleAsComponent/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": [
        {
          "@id": "Component/"
        },
        {
          "@id": "Resource/"
        }
      ],
      "rdfs:label": "Handle As Component",
      "rdfs:comment": "A handle considered as a mesh component in its own right, addressable via IRI and subject to mesh operations."
    },
    {
      "@id": "DocumentationResource/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": {
        "@id": "FileResource/"
      },
      "rdfs:label": "Documentation Resource",
      "rdfs:comment": "Documentation files that provide human-readable information about resources."
    },
    {
      "@id": "ResourcePage/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": {
        "@id": "DocumentationResource/"
      },
      "rdfs:label": "Resource Page",
      "rdfs:comment": "Human-readable index.html pages that provide dereferenceability for NamingResources."
    },
    {
      "@id": "Changelog/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": {
        "@id": "DocumentationResource/"
      },
      "rdfs:label": "Changelog",
      "rdfs:comment": "Changelog files that document version history and changes for resources."
    },
    {
      "@id": "Readme/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": {
        "@id": "DocumentationResource/"
      },
      "rdfs:label": "Readme",
      "rdfs:comment": "Readme files that provide introductory documentation and usage information."
    },
    {
      "@id": "hasDocumentationResource/",
      "@type": "owl:ObjectProperty",
      "schema:domainIncludes": {
        "@id": "NamingResource/"
      },
      "schema:rangeIncludes": {
        "@id": "DocumentationResource/"
      },
      "rdfs:label": "has DocumentationResource",
      "rdfs:comment": "Associates a resource with a documentation resource."
    },
    {
      "@id": "hasResourcePage/",
      "@type": "owl:ObjectProperty",
      "schema:domainIncludes": {
        "@id": "NamingResource/"
      },
      "schema:rangeIncludes": {
        "@id": "ResourcePage/"
      },
      "rdfs:label": "has ResourcePage",
      "rdfs:comment": "Associates a NamingResource with its index.html resource page file."
    },
    {
      "@id": "hasReadme/",
      "@type": "owl:ObjectProperty",
      "schema:domainIncludes": {
        "@id": "NamingResource/"
      },
      "schema:rangeIncludes": {
        "@id": "Readme/"
      },
      "rdfs:label": "has Readme",
      "rdfs:comment": "Associates a NamingResource with its README documentation file."
    },
    {
      "@id": "hasChangelog/",
      "@type": "owl:ObjectProperty",
      "schema:domainIncludes": {
        "@id": "NamingResource/"
      },
      "schema:rangeIncludes": {
        "@id": "Changelog/"
      },
      "rdfs:label": "has Changelog",
      "rdfs:comment": "Associates a NamingResource with its changelog documentation file."
    },
    {
      "@id": "Flow/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": [
        {
          "@id": "Component/"
        }
      ],
      "rdfs:label": "Flow",
      "rdfs:comment": "Evolvable containers for knop metadata, reference data, payload data, configuration, and opslogs."
    },
    {
      "@id": "FlowWithVersions/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": [
        {
          "@id": "dcat:DatasetSeries"
        },
        {
          "@id": "Flow/"
        }
      ],
      "rdfs:label": "Flow with versions",
      "rdfs:comment": "Evolving DatasetSeries that have at least one version slice."
    },
    {
      "@id": "KnopMetadataFlow/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": {
        "@id": "Flow/"
      },
      "rdfs:label": "Metadata Flow",
      "rdfs:comment": "Flow containing metadata about the mesh knop itself."
    },
    {
      "@id": "ReferenceFlow/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": {
        "@id": "Flow/"
      },
      "rdfs:label": "Reference Flow",
      "rdfs:comment": "Flow containing reference data about the thing or concept referred to by the mesh knop."
    },
    {
      "@id": "PayloadFlow/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": {
        "@id": "Flow/"
      },
      "rdfs:label": "Payload flow",
      "rdfs:comment": "Flow containing the dataset payload associated with the mesh knop."
    },
    {
      "@id": "KnopConfigFlow/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": {
        "@id": "Flow/"
      },
      "rdfs:label": "knop config flow",
      "rdfs:comment": "Abstract flow for mesh knop configuration data."
    },
    {
      "@id": "InheritableKnopConfigFlow/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": {
        "@id": "KnopConfigFlow/"
      },
      "rdfs:label": "Inheritable knop config flow",
      "rdfs:comment": "knop configuration flow whose settings can be inherited by child knops."
    },
    {
      "@id": "LocalKnopConfigFlow/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": {
        "@id": "KnopConfigFlow/"
      },
      "rdfs:label": "Local knop config flow",
      "rdfs:comment": "knop configuration flow containing persisted local settings for this specific knop. Combined with inherited config at runtime to produce OperationalKnopConfig."
    },
    {
      "@id": "OpslogFlow/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": {
        "@id": "Flow/"
      },
      "rdfs:label": "Opslog flow",
      "rdfs:comment": "flow for tracking operations and changes within the knop."
    },
    {
      "@id": "hasFlow/",
      "@type": "owl:ObjectProperty",
      "rdfs:subPropertyOf": {
        "@id": "hasComponent/"
      },
      "rdfs:domain": {
        "@id": "Knop/"
      },
      "rdfs:range": {
        "@id": "Flow/"
      },
      "rdfs:label": "has Flow",
      "rdfs:comment": "Links mesh knops to their flows (metadata, reference, data, config)."
    },
    {
      "@id": "hasKnopMetadataFlow/",
      "@type": "owl:ObjectProperty",
      "rdfs:subPropertyOf": {
        "@id": "hasFlow/"
      },
      "rdfs:domain": {
        "@id": "Knop/"
      },
      "rdfs:range": {
        "@id": "KnopMetadataFlow/"
      },
      "rdfs:label": "has KnopMetadataFlow",
      "rdfs:comment": "Links mesh knops to their metadata flow."
    },
    {
      "@id": "hasReferenceFlow/",
      "@type": "owl:ObjectProperty",
      "rdfs:subPropertyOf": {
        "@id": "hasFlow/"
      },
      "rdfs:domain": {
        "@id": "Knop/"
      },
      "rdfs:range": {
        "@id": "ReferenceFlow/"
      },
      "rdfs:label": "has ReferenceFlow",
      "rdfs:comment": "Links mesh knops to their reference flow."
    },
    {
      "@id": "hasPayloadFlow/",
      "@type": "owl:ObjectProperty",
      "rdfs:subPropertyOf": {
        "@id": "hasFlow/"
      },
      "rdfs:domain": {
        "@id": "Knop/"
      },
      "rdfs:range": {
        "@id": "PayloadFlow/"
      },
      "rdfs:label": "has PayloadFlow",
      "rdfs:comment": "Links mesh knops to their payload flow."
    },
    {
      "@id": "FlowSlice/",
      "@type": "owl:Class",
      "rdfs:subClassOf": [
        {
          "@id": "NamingResource/"
        },
        {
          "@id": "dcat:Dataset"
        }
      ],
      "rdfs:label": "Flow Slice",
      "rdfs:comment": "One concrete dataset instance belonging to a Flow: a committed version, the default slice under '_default/', or the mutable working slice under '_working/'. A FlowSlice represents the Flow's data as a whole at a particular stage, not a subset of it."
    },
    {
      "@id": "VersionSlice/",
      "@type": "owl:Class",
      "rdfs:subClassOf": {
        "@id": "FlowSlice/"
      },
      "rdfs:label": "Version Slice",
      "rdfs:comment": "Immutable, committed flow slice (dataset state), typically materialized as a timestamped weave directory. Versioned flows have one Version per committed weave; unversioned flows may have none."
    },
    {
      "@id": "DefaultSlice/",
      "@type": "owl:Class",
      "rdfs:subClassOf": {
        "@id": "FlowSlice/"
      },
      "rdfs:label": "Default Slice",
      "rdfs:comment": "The Flow's default dataset slice under the '_default/' directory. For versioned flows, its content mirrors the latest Version. For unversioned flows, it is the only authoritative dataset state. For deversioned flows, it is the current dataset state after the last weave."
    },
    {
      "@id": "WorkingSlice/",
      "@type": "owl:Class",
      "rdfs:subClassOf": {
        "@id": "FlowSlice/"
      },
      "rdfs:label": "Working Slice",
      "rdfs:comment": "Mutable staging slice for a Flow, mapped to the '_working/' directory. Used for edits before weave. Not a committed dataset and normally not exposed in version history or as-of queries."
    },
    {
      "@id": "hasFlowSlice/",
      "@type": "owl:ObjectProperty",
      "rdfs:subPropertyOf": {
        "@id": "hasComponent/"
      },
      "rdfs:domain": {
        "@id": "Flow/"
      },
      "rdfs:range": {
        "@id": "FlowSlice/"
      },
      "rdfs:label": "has flow slice",
      "rdfs:comment": "Associates a Flow with one of its slices (default, version, or working)."
    },
    {
      "@id": "hasDefaultSlice/",
      "@type": "owl:ObjectProperty",
      "rdfs:subPropertyOf": {
        "@id": "dcat:seriesMember/"
      },
      "rdfs:domain": {
        "@id": "Flow/"
      },
      "rdfs:range": {
        "@id": "DefaultSlice/"
      },
      "rdfs:label": "has default slice",
      "rdfs:comment": "Links a Flow to its default slice under '_default/'. Each Flow must have exactly one DefaultSlice."
    },
    {
      "@id": "hasVersionSlice/",
      "@type": "owl:ObjectProperty",
      "rdfs:subPropertyOf": [
        {
          "@id": "hasFlowSlice/"
        },
        {
          "@id": "prov:specializationOf"
        }
      ],
      "rdfs:domain": {
        "@id": "Flow/"
      },
      "rdfs:range": {
        "@id": "VersionSlice/"
      },
      "rdfs:label": "has version slice",
      "rdfs:comment": "Associates a Flow with one of its committed version slices and specializes the Flow as a DatasetSeries into this particular Version dataset."
    },
    {
      "@id": "Distribution/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": [
        {
          "@id": "dcat:Distribution"
        },
        {
          "@id": "FileResource/"
        },
        {
          "@id": "Component/"
        }
      ],
      "rdfs:label": "Distribution",
      "rdfs:comment": "A distribution (file) that contains the RDF data for a FlowSlice (version, default slice, or working slice)."
    },
    {
      "@id": "hasDistribution/",
      "@type": "owl:ObjectProperty",
      "rdfs:subPropertyOf": {
        "@id": "dcat:distribution"
      },
      "schema:domainIncludes": {
        "@id": "FlowSlice/"
      },
      "schema:rangeIncludes": {
        "@id": "Distribution/"
      },
      "rdfs:label": "has Distribution",
      "rdfs:comment": "Associates a FlowSlice with its data distribution files."
    },
    {
      "@id": "hasCurrentSlice/",
      "@type": "owl:ObjectProperty",
      "rdfs:subPropertyOf": {
        "@id": "hasFlowSlice/"
      },
      "rdfs:domain": {
        "@id": "Flow/"
      },
      "schema:rangeIncludes": [
        {
          "@id": "VersionSlice/"
        },
        {
          "@id": "DefaultSlice/"
        }
      ],
      "rdfs:label": "hasCurrentSlice",
      "rdfs:comment": "Associates a Flow with its current FlowSlice."
    },
    {
      "@id": "previousVersion/",
      "@type": "owl:ObjectProperty",
      "rdfs:subPropertyOf": {
        "@id": "dcat:previousVersion"
      },
      "rdfs:label": "previous version",
      "rdfs:comment": "Links to the immediately-preceeding version of this FlowSlice.",
      "schema:domainIncludes": {
        "@id": "FlowSlice/"
      },
      "schema:rangeIncludes": {
        "@id": "Version/"
      }
    },
    {
      "@id": "VersioningState/",
      "@type": "owl:Class",
      "rdfs:label": "Versioning State",
      "rdfs:comment": "Enumeration of possible versioning states for a flow.",
      "owl:oneOf": {
        "@list": [
          {
            "@id": "VersioningState/versioned/"
          },
          {
            "@id": "VersioningState/unversioned/"
          },
          {
            "@id": "VersioningState/deversioned/"
          }
        ]
      }
    },
    {
      "@id": "VersioningState/versioned/",
      "@type": [
        "owl:NamedIndividual",
        "VersioningState/"
      ],
      "rdfs:label": "Versioned",
      "rdfs:comment": "The flow maintains versions with distinct version keys."
    },
    {
      "@id": "VersioningState/unversioned/",
      "@type": [
        "owl:NamedIndividual",
        "VersioningState/"
      ],
      "rdfs:label": "unversioned",
      "rdfs:comment": "The flow does not have any Versions; only a WorkingSlice and DefaultSlice exist."
    },
    {
      "@id": "VersioningState/deversioned/",
      "@type": [
        "owl:NamedIndividual",
        "VersioningState/"
      ],
      "rdfs:label": "deversioned",
      "rdfs:comment": "The flow previously maintained versions but has been deversioned; historical versions may still exist."
    },
    {
      "@id": "versioningState/",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "versioning state",
      "rdfs:comment": "The versioning state of a flow.",
      "schema:domainIncludes": {
        "@id": "Flow/"
      },
      "schema:rangeIncludes": {
        "@id": "VersioningState/"
      }
    },
    {
      "@id": "weaveLabel/",
      "@type": "owl:DatatypeProperty",
      "schema:domainIncludes": [
        {
          "@id": "Flow/"
        },
        {
          "@id": "Version/"
        }
      ],
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "rdfs:label": "weave label",
      "rdfs:comment": "Human-readable label for a weave run (e.g., '2025-11-24_0142_07' or '2025-11-24_0142_15'). Format is YYYY-MM-DD_HHMM_SS with second-level precision. All versions created by the same weave share the same weaveLabel. Not guaranteed to be globally unique across all meshes."
    },
    {
      "@id": "versionNumber/",
      "@type": "owl:DatatypeProperty",
      "schema:domainIncludes": {
        "@id": "Version/"
      },
      "schema:rangeIncludes": {
        "@id": "xsd:integer"
      },
      "rdfs:label": "sequence number",
      "rdfs:comment": "Monotonic integer identifying this version within its versioned Flow. 1 for the first committed version of a flow, 2 for the next, etc. Used for ordering and for detecting gaps or compaction at the flow level."
    },
    {
      "@id": "hasLocalConfigFlow/",
      "@type": "owl:ObjectProperty",
      "schema:domainIncludes": {
        "@id": "KnopHandle/"
      },
      "schema:rangeIncludes": {
        "@id": "LocalKnopConfigFlow/"
      },
      "rdfs:label": "local config",
      "rdfs:comment": "Links mesh knops to their local config flow."
    },
    {
      "@id": "hasInheritableConfigFlow/",
      "@type": "owl:ObjectProperty",
      "schema:domainIncludes": {
        "@id": "KnopHandle/"
      },
      "schema:rangeIncludes": {
        "@id": "InheritableKnopConfigFlow/"
      },
      "rdfs:label": "inheritable config",
      "rdfs:comment": "Links mesh knops to their operational config flow."
    },
    {
      "@id": "parentKnop/",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "parent knop",
      "rdfs:comment": "indicates that this knop is contained in another knop. Should only be used in OperationalKnopConfig.",
      "schema:domainIncludes": {
        "@id": "sflo:Handle/"
      },
      "schema:rangeIncludes": {
        "@id": "sflo:Handle/"
      }
    },
    {
      "@id": "rootKnop/",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "root knop",
      "rdfs:comment": "indicates that this is a root knop. Should only be used in OperationalKnopConfig.",
      "schema:rangeIncludes": {
        "@id": "xsd:boolean"
      }
    },
    {
      "@id": "disjoint/FileVsNamingResources",
      "@type": "owl:AllDisjointClasses",
      "owl:members": {
        "@list": [
          {
            "@id": "FileResource/"
          },
          {
            "@id": "NamingResource/"
          }
        ]
      }
    },
    {
      "@id": "disjoint/KnopStructuralRoles",
      "@type": "owl:AllDisjointClasses",
      "owl:members": {
        "@list": [
          {
            "@id": "BareKnop/"
          },
          {
            "@id": "ReferenceKnop/"
          },
          {
            "@id": "PayloadKnop/"
          }
        ]
