{
  "@context": {
    "@base": "https://semantic-flow.github.io/ontology/semantic-flow/",
    "owl": "http://www.w3.org/2002/07/owl#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "dcterms": "http://purl.org/dc/terms/",
    "dcat": "http://www.w3.org/ns/dcat#",
    "sh": "http://www.w3.org/ns/shacl#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "prov": "http://www.w3.org/ns/prov#",
    "schema": "https://schema.org/",
    "vann": "http://purl.org/vocab/vann/"
  },
  "@graph": [
    {
      "@id": "/",
      "@type": "owl:Ontology",
      "dcterms:title": "Semantic Flow Ontology",
      "dcterms:description": "The Semantic Flow ontology defines the classes and properties used for defining and describing semantic meshes.",
      "dcterms:creator": {
        "@id": "https://djradon.github.io/ns/djradon/"
      },
      "owl:versionInfo": "0.1.0",
      "owl:versionIRI": {
        "@id": "https://semantic-flow.github.io/ontology/semantic-flow/_payload/_default/sflo.ttl"
      },
      "vann:preferredNamespacePrefix": "sflo",
      "vann:preferredNamespaceUri": "https://semantic-flow.github.io/ontology/semantic-flow/"
    },
    {
      "@id": "PlatformElement/",
      "@type": "rdfs:Class",
      "rdfs:label": "Platform Element",
      "rdfs:comment": "Abstract base class for all Semantic Flow resources. Provides fundamental identification and documentation affordances."
    },
    {
      "@id": "FileResource/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": {
        "@id": "Component/"
      },
      "rdfs:label": "File Resource",
      "rdfs:comment": "A resource that locates and corresponds directly to a file."
    },
    {
      "@id": "NamingResource/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": {
        "@id": "PlatformElement/"
      },
      "rdfs:label": "Naming Resource",
      "rdfs:comment": "A resource that establishes a name, optionally denoting a non-file thing. Naming resources' IRIs end with a slash and may or may not have a referent."
    },
    {
      "@id": "AssetTree/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": [
        {
          "@id": "Component/"
        },
        {
          "@id": "NamingResource/"
        }
      ],
      "rdfs:label": "AssetTree",
      "rdfs:comment": "Asset trees contain collections of files and directories that are not PlatformElements. Can be contained in any KnopNode."
    },
    {
      "@id": "Knop/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": {
        "@id": "NamingResource/"
      },
      "rdfs:label": "Mesh Knop",
      "rdfs:comment": "Structural units that provide identifiers for things, may act as namespace segments to organize/contain other resources, may contain reference data about their referents, and may have a payload dataset. Knops form the structure of the mesh."
    },
    {
      "@id": "BareKnop/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": {
        "@id": "Knop/"
      },
      "rdfs:label": "Namespace",
      "rdfs:comment": "Bare knops - containers for organizing other mesh knops. Knop identifiers refer to the knops themselves as organizational units."
    },
    {
      "@id": "ReferenceKnop/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": {
        "@id": "Knop/"
      },
      "rdfs:label": "Reference",
      "rdfs:comment": "Knops that refer to some external 'thing' or concept other than themselves."
    },
    {
      "@id": "PayloadKnop/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": {
        "@id": "Knop/"
      },
      "rdfs:label": "Data",
      "rdfs:comment": "Knops that contain and refer to a sflo:DigitalArtifact via a payload flow."
    },
    {
      "@id": "hasComponent/",
      "@type": "owl:ObjectProperty",
      "rdfs:domain": {
        "@id": "Knop/"
      },
      "rdfs:range": {
        "@id": "Component/"
      },
      "rdfs:label": "has component",
      "rdfs:comment": "Associates knops with their supporting components."
    },
    {
      "@id": "isKnopFor/",
      "@type": "owl:ObjectProperty",
      "owl:inverseOf": {
        "@id": "hasKnop/"
      },
      "rdfs:domain": {
        "@id": "HandleAsComponent/"
      },
      "rdfs:range": {
        "@id": "Knop/"
      },
      "rdfs:label": "is handle for",
      "rdfs:comment": "Indicates that this handle component provides identification for a specific knop."
    },
    {
      "@id": "hasKnop/",
      "@type": "owl:ObjectProperty",
      "owl:inverseOf": {
        "@id": "isKnopFor/"
      },
      "rdfs:domain": {
        "@id": "rdfs:Resource/"
      },
      "rdfs:range": {
        "@id": "Knop/"
      },
      "rdfs:label": "has Knop",
      "rdfs:comment": "Associates a referent (a thing or concept worth identifying) with its Knop."
    },
    {
      "@id": "Component/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": {
        "@id": "Resource/"
      },
      "rdfs:label": "Component",
      "rdfs:comment": "Resources that provide specific mesh functionality and support knops."
    },
    {
      "@id": "KnopNode/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": {
        "@id": "NamingResource/"
      },
      "rdfs:label": "Knop node",
      "rdfs:comment": "A specific location of a Knop."
    },
    {
      "@id": "DocumentationResource/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": {
        "@id": "FileResource/"
      },
      "rdfs:label": "Documentation Resource",
      "rdfs:comment": "Documentation files that provide human-readable information about resources."
    },
    {
      "@id": "ResourcePage/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": {
        "@id": "DocumentationResource/"
      },
      "rdfs:label": "Resource Page",
      "rdfs:comment": "Human-readable index.html pages that provide dereferenceability for NamingResources."
    },
    {
      "@id": "Changelog/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": {
        "@id": "DocumentationResource/"
      },
      "rdfs:label": "Changelog",
      "rdfs:comment": "Changelog files that document the history and changes for resources."
    },
    {
      "@id": "Readme/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": {
        "@id": "DocumentationResource/"
      },
      "rdfs:label": "Readme",
      "rdfs:comment": "Readme files that provide introductory documentation and usage information."
    },
    {
      "@id": "hasDocumentationResource/",
      "@type": "owl:ObjectProperty",
      "schema:domainIncludes": {
        "@id": "Knop/"
      },
      "schema:rangeIncludes": {
        "@id": "DocumentationResource/"
      },
      "rdfs:label": "has DocumentationResource",
      "rdfs:comment": "Associates a resource with a documentation resource."
    },
    {
      "@id": "hasResourcePage/",
      "@type": "owl:ObjectProperty",
      "schema:domainIncludes": {
        "@id": "NamingResource/"
      },
      "schema:rangeIncludes": {
        "@id": "ResourcePage/"
      },
      "rdfs:label": "has ResourcePage",
      "rdfs:comment": "Associates a NamingResource with its index.html resource page file."
    },
    {
      "@id": "hasReadme/",
      "@type": "owl:ObjectProperty",
      "schema:domainIncludes": {
        "@id": "Knop/"
      },
      "schema:rangeIncludes": {
        "@id": "Readme/"
      },
      "rdfs:label": "has Readme",
      "rdfs:comment": "Associates a Knop with its README documentation file."
    },
    {
      "@id": "hasChangelog/",
      "@type": "owl:ObjectProperty",
      "schema:domainIncludes": {
        "@id": "Knop/"
      },
      "schema:rangeIncludes": {
        "@id": "Changelog/"
      },
      "rdfs:label": "has Changelog",
      "rdfs:comment": "Associates a Knop with its changelog documentation file."
    },
    {
      "@id": "Flow/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": [
        {
          "@id": "Component/"
        }
      ],
      "rdfs:label": "Flow",
      "rdfs:comment": "Evolvable containers for knop metadata, reference data, payload data, and configuration."
    },
    {
      "@id": "KnopMetadataFlow/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": {
        "@id": "Flow/"
      },
      "rdfs:label": "Metadata Flow",
      "rdfs:comment": "Flow containing metadata about the mesh knop itself."
    },
    {
      "@id": "ReferenceFlow/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": {
        "@id": "Flow/"
      },
      "rdfs:label": "Reference Flow",
      "rdfs:comment": "Flow containing reference data about the thing or concept referred to by the mesh knop."
    },
    {
      "@id": "PayloadFlow/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": {
        "@id": "Flow/"
      },
      "rdfs:label": "Payload flow",
      "rdfs:comment": "Flow containing version of the sflo:DigitalArtifact identified by a PayloadKnop."
    },
    {
      "@id": "KnopConfigFlow/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": {
        "@id": "Flow/"
      },
      "rdfs:label": "knop config flow",
      "rdfs:comment": "Abstract flow for mesh knop configuration data."
    },
    {
      "@id": "InheritableKnopConfigFlow/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": {
        "@id": "KnopConfigFlow/"
      },
      "rdfs:label": "Inheritable knop config flow",
      "rdfs:comment": "knop configuration flow whose settings can be inherited by child knops."
    },
    {
      "@id": "LocalKnopConfigFlow/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": {
        "@id": "KnopConfigFlow/"
      },
      "rdfs:label": "Local knop config flow",
      "rdfs:comment": "knop configuration flow containing persisted local settings for this specific knop. Combined with inherited config at runtime to produce OperationalKnopConfig."
    },
    {
      "@id": "hasFlow/",
      "@type": "owl:ObjectProperty",
      "rdfs:subPropertyOf": {
        "@id": "hasComponent/"
      },
      "rdfs:domain": {
        "@id": "Knop/"
      },
      "rdfs:range": {
        "@id": "Flow/"
      },
      "rdfs:label": "has Flow",
      "rdfs:comment": "Links mesh knops to their flows (metadata, reference, data, config)."
    },
    {
      "@id": "hasKnopMetadataFlow/",
      "@type": "owl:ObjectProperty",
      "rdfs:subPropertyOf": {
        "@id": "hasFlow/"
      },
      "rdfs:domain": {
        "@id": "Knop/"
      },
      "rdfs:range": {
        "@id": "KnopMetadataFlow/"
      },
      "rdfs:label": "has KnopMetadataFlow",
      "rdfs:comment": "Links mesh knops to their metadata flow."
    },
    {
      "@id": "hasReferenceFlow/",
      "@type": "owl:ObjectProperty",
      "rdfs:subPropertyOf": {
        "@id": "hasFlow/"
      },
      "rdfs:domain": {
        "@id": "Knop/"
      },
      "rdfs:range": {
        "@id": "ReferenceFlow/"
      },
      "rdfs:label": "has ReferenceFlow",
      "rdfs:comment": "Links mesh knops to their reference flow."
    },
    {
      "@id": "hasPayloadFlow/",
      "@type": "owl:ObjectProperty",
      "rdfs:subPropertyOf": {
        "@id": "hasFlow/"
      },
      "rdfs:domain": {
        "@id": "Knop/"
      },
      "rdfs:range": {
        "@id": "PayloadFlow/"
      },
      "rdfs:label": "has PayloadFlow",
      "rdfs:comment": "Links mesh knops to their payload flow."
    },
    {
      "@id": "FlowSlice/",
      "@type": "owl:Class",
      "rdfs:subClassOf": [
        {
          "@id": "NamingResource/"
        },
        {
          "@id": "dcat:Dataset"
        }
      ],
      "rdfs:label": "Flow Slice",
      "rdfs:comment": "One concrete dataset instance belonging to a Flow. A FlowSlice represents the Flow's data as a whole at a particular stage, not a subset of it."
    },
    {
      "@id": "hasFlowSlice/",
      "@type": "owl:ObjectProperty",
      "rdfs:subPropertyOf": {
        "@id": "hasComponent/"
      },
      "rdfs:domain": {
        "@id": "Flow/"
      },
      "rdfs:range": {
        "@id": "FlowSlice/"
      },
      "rdfs:label": "has flow slice",
      "rdfs:comment": "Associates a Flow with one of its slices."
    },
    {
      "@id": "Distribution/",
      "@type": "rdfs:Class",
      "rdfs:subClassOf": [
        {
          "@id": "dcat:Distribution"
        },
        {
          "@id": "FileResource/"
        }
      ],
      "rdfs:label": "Distribution",
      "rdfs:comment": "A distribution (file) that contains the content for a FlowSlice."
    },
    {
      "@id": "hasDistribution/",
      "@type": "owl:ObjectProperty",
      "rdfs:subPropertyOf": {
        "@id": "dcat:distribution"
      },
      "schema:domainIncludes": {
        "@id": "FlowSlice/"
      },
      "schema:rangeIncludes": {
        "@id": "Distribution/"
      },
      "rdfs:label": "has Distribution",
      "rdfs:comment": "Associates a FlowSlice with its data distribution files."
    },
    {
      "@id": "hasCurrentSlice/",
      "@type": "owl:ObjectProperty",
      "rdfs:subPropertyOf": {
        "@id": "hasFlowSlice/"
      },
      "rdfs:domain": {
        "@id": "Flow/"
      },
      "schema:rangeIncludes": [
        {
          "@id": "FlowSlice/"
        }
      ],
      "rdfs:label": "has current slice",
      "rdfs:comment": "Associates a Flow with its current FlowSlice."
    },
    {
      "@id": "previousSlice/",
      "@type": "owl:ObjectProperty",
      "rdfs:subPropertyOf": {
        "@id": "dcat:previousVersion"
      },
      "rdfs:label": "previous slice",
      "rdfs:comment": "Links to the immediately-preceeding slice of this FlowSlice.",
      "schema:domainIncludes": {
        "@id": "FlowSlice/"
      },
      "schema:rangeIncludes": {
        "@id": "FlowSlice/"
      }
    },
    {
      "@id": "weaveLabel/",
      "@type": "owl:DatatypeProperty",
      "schema:domainIncludes": [
        {
          "@id": "FlowSlice/"
        }
      ],
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "rdfs:label": "weave label",
      "rdfs:comment": "Human-readable label corresponding to a weave run (e.g., '2025-11-24_0142_07' or '2025-11-24_0142_15'). Format is YYYY-MM-DD_HHMM_SS with second-level precision. All slices created by the same weave share the same weaveLabel. Not likely to be unique across meshes."
    },
    {
      "@id": "sliceSequence/",
      "@type": "owl:DatatypeProperty",
      "schema:domainIncludes": {
        "@id": "FlowSlice/"
      },
      "schema:rangeIncludes": {
        "@id": "xsd:integer"
      },
      "rdfs:label": "slice sequence",
      "rdfs:comment": "Monotonic integer providing the sequence number of this slice within its Flow. Usually 1 for the first-woven slice of a flow, 2 for the next, etc. Used for ordering and for detecting gaps or compaction at the flow level."
    },
    {
      "@id": "hasLocalConfigFlow/",
      "@type": "owl:ObjectProperty",
      "schema:domainIncludes": {
        "@id": "Knop/"
      },
      "schema:rangeIncludes": {
        "@id": "LocalKnopConfigFlow/"
      },
      "rdfs:label": "local config",
      "rdfs:comment": "Links mesh knops to their local config flow."
    },
    {
      "@id": "hasInheritableConfigFlow/",
      "@type": "owl:ObjectProperty",
      "schema:domainIncludes": {
        "@id": "Knop/"
      },
      "schema:rangeIncludes": {
        "@id": "InheritableKnopConfigFlow/"
      },
      "rdfs:label": "inheritable config",
      "rdfs:comment": "Links mesh knops to their operational config flow."
    },
    {
      "@id": "anticipatedAbsoluteIri/",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "anticipated absolute IRI",
      "rdfs:comment": "The anticipated absolute IRI of this knop within a published mesh, based on its Knop identifier, its path location, and the hosting platform."
    },
    {
      "@id": "hasKnopNode/",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "has knop node",
      "rdfs:comment": "Associates a KnopNode with its Knop.",
      "schema:domainIncludes": {
        "@id": "Knop/"
      },
      "schema:rangeIncludes": {
        "@id": "KnopNode/"
      }
    },
    {
      "@id": "hasActiveKnopNode/",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "has active knop node",
      "rdfs:comment": "Associates a Knop with its active KnopNode.",
      "rdfs:subPropertyOf": {
        "@id": "hasKnopNode/"
      },
      "schema:domainIncludes": {
        "@id": "Knop/"
      },
      "schema:rangeIncludes": {
        "@id": "KnopNode/"
      }
    },
    {
      "@id": "peerKnopNode/",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "is peer KnopNode",
      "rdfs:comment": "Associates a KnopNode with another KnopNode that is part of the same conceptual knop.",
      "schema:domainIncludes": {
        "@id": "KnopNode/"
      },
      "schema:rangeIncludes": {
        "@id": "KnopNode/"
      }
    },
    {
      "@id": "activePeerKnopNode/",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "active peer KnopNode",
      "rdfs:comment": "Associates a KnopNode with the currently active KnopNode for a Knop.",
      "rdfs:subPropertyOf": {
        "@id": "peerKnopNode/"
      },
      "schema:domainIncludes": {
        "@id": "KnopNode/"
      },
      "schema:rangeIncludes": {
        "@id": "KnopNode/"
      }
    },
    {
      "@id": "isActiveNode/",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "is active node",
      "rdfs:comment": "asserts that a KnopNode is active.",
      "schema:domainIncludes": {
        "@id": "KnopNode/"
      },
      "schema:rangeIncludes": {
        "@id": "xsd:boolean"
      }
    },
    {
      "@id": "parentKnop/",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "parent knop",
      "rdfs:comment": "indicates that this knop is contained in another knop. Should only be used in OperationalKnopConfig.",
      "schema:domainIncludes": {
        "@id": "sflo:Handle/"
      },
      "schema:rangeIncludes": {
        "@id": "sflo:Handle/"
      }
    },
    {
      "@id": "rootKnop/",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "root knop",
      "rdfs:comment": "indicates that this is a root knop. Should only be used in OperationalKnopConfig.",
      "schema:rangeIncludes": {
        "@id": "xsd:boolean"
      }
    },
    {
      "@id": "disjoint/FileVsNamingResources",
      "@type": "owl:AllDisjointClasses",
      "owl:members": {
        "@list": [
          {
            "@id": "FileResource/"
          },
          {
            "@id": "NamingResource/"
          }
        ]
      }
    },
    {
      "@id": "disjoint/KnopStructuralRoles",
      "@type": "owl:AllDisjointClasses",
      "owl:members": {
        "@list": [
          {
            "@id": "BareKnop/"
          },
          {
            "@id": "ReferenceKnop/"
          },
          {
            "@id": "PayloadKnop/"
          }
        ]
      }
    },
    {
      "@id": "disjoint/ComponentResources",
      "@type": "owl:AllDisjointClasses",
      "owl:members": {
        "@list": [
          {
            "@id": "HandleAsComponent/"
          },
          {
            "@id": "DocumentationResource/"
          },
          {
            "@id": "AssetTree/"
          },
          {
            "@id": "Flow/"
          },
          {
            "@id": "FlowSlice/"
          },
          {
            "@id": "Distribution/"
          }
        ]
      }
    },
    {
      "@id": "disjoint/FlowTypes",
      "@type": "owl:AllDisjointClasses",
      "owl:members": {
        "@list": [
          {
            "@id": "KnopMetadataFlow/"
          },
          {
            "@id": "ReferenceFlow/"
          },
          {
            "@id": "PayloadFlow/"
          },
          {
            "@id": "InheritableKnopConfigFlow/"
          },
          {
            "@id": "LocalKnopConfigFlow/"
          }
        ]
      }
    }
  ]
}
